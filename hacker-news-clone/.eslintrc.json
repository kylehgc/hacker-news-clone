{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "jest/globals": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    // "parser": "@babel/eslint-parser",
    "plugins": [
        "react",
        "jest",
        "jsx"
    ],
    "rules": {
        "indent":[2,2],
        "react/react-in-jsx-scope": "off"
    }
}
